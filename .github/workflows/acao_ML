
truename: Acao_ML

on: 
 pull_requests:
   branches:
    - master

jobs:
  check-bats-version:
    runs-on: ubuntu-latest
    steps:      
      - uses: actions/checkout@v2
      - name: Install node.js
        uses: actions/setup-node@v2
        with:
          node-version: '14.x'
      - run: iarn
      - run: iarn acao_ML
      - run: npm install -g bats
      - run: bats -v
      env: 
       CI:true
